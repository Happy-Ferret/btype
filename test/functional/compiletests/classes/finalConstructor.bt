object Foo {
    int:x;
    final new() {
        # `x` is final, so it can only be set from the constructor
        # `getX` is final, so it can be called by the constructor even though
        # the constructor is marked `final`.
        self.x = self.getX();
    }
    final int:getX() {
        return 42;
    }
}

func main() {
    var foo = new Foo();
}
export main;
